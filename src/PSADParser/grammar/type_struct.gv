digraph TYPE_STRUCT{
    start [label=TYPE_STRUCT shape=plaintext]
    struct [label=struct shape=oval]
    open [label="(" shape=oval]
    type [label=TYPE shape=box]
    statement [label=STATEMENT shape=box]
    close [label=")" shape=oval]
    end [label="" shape=point]

    start -> struct
    struct -> open
    open -> type
    open -> statement
    type -> close
    statement -> close
    close -> end
}
